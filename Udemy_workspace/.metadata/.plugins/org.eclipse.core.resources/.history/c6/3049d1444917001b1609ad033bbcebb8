
public class NumberInWord {

	public static void printNumberInWord(int num) {
		
		switch(num) {
			
		case 0;
			System.out.println("Zero");
			break;
		case 0;
			System.out.println("Zero");
			break;
		case 0;
			System.out.println("Zero");
			break;
		case 0;
			System.out.println("Zero");
			break;
		case 0;
			System.out.println("Zero");
			break;
		}
	}
}
