
public class NumberOfDaysInMonth {
	
	public static void main(String[] args) {
		isLeapYear(2020);
	}
	
	public static boolean isLeapYear(int year) {
		
		if(year>=1 && year<=9999) {
			if(year%400)
				return true;
			else
				return false;
				if(year%100)
					return true;
				else
					return false;
					if(year%4)
						return true;
					else
						return false;
			return false;
						
		}
		else
			return false;
	}

}
