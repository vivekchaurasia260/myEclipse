
public class DayOfTheWeek {
	public static void main(String[] args) {
		
	}
	
	public static void printDayOfTheWeek(int day) {
		
		switch(day) {
		case 0:
			System.out.println("Sunday");
			break;
		case 1:
			System.out.println("Monday");
			break;
		case 2:
			System.out.println("Sunday");
			break;
		}
			
	}
	
}
 